<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<HBox alignment="TOP_CENTER" prefHeight="616.0" prefWidth="976.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Clientes.ClienteFXMLController">
   <children>
      <AnchorPane id="AnchorPane" prefHeight="496.0" prefWidth="976.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="338.0" layoutY="14.0" text="Bem-Vindo a Área de Cadastro de Clientes">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <VBox alignment="TOP_CENTER" layoutX="10.0" layoutY="48.0" prefHeight="289.0" prefWidth="957.0" AnchorPane.topAnchor="48.0">
               <children>
                  <Label text="Nome" />
                  <TextField fx:id="txt_nome" />
                  <Label text="Endereço" />
                  <TextField fx:id="txt_endereco" />
                  <Label text="Cpf" />
                  <TextField fx:id="txt_cpf" />
                  <Label text="Projeto" />
                  <TextField fx:id="txt_projeto" />
                  <Label text="Protocolo" />
                  <Label fx:id="lbl_protocolo" alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="91.0" textAlignment="CENTER" textFill="#174bd1">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <Label text="Telefone" />
                  <TextField fx:id="txt_fone" />
                  <Label text="Email" />
                  <TextField fx:id="txt_mail" />
               </children>
            </VBox>
            <HBox layoutX="14.0" layoutY="349.0" prefHeight="37.0" prefWidth="951.0">
               <children>
                  <Button fx:id="bt_cadastrar" mnemonicParsing="false" onAction="#Cadastrar" prefHeight="36.0" prefWidth="87.0" text="Cadastrar" />
                  <Button fx:id="bt_alterar" mnemonicParsing="false" onAction="#Alterar" prefHeight="37.0" prefWidth="83.0" text="Alterar" />
                  <Button fx:id="bt_sair" mnemonicParsing="false" onAction="#Sair" prefHeight="37.0" prefWidth="86.0" text="Sair" />
               </children>
            </HBox>
            <TableView fx:id="tb_clientes" layoutX="14.0" layoutY="386.0" onMouseClicked="#Listar" prefHeight="200.0" prefWidth="951.0">
              <columns>
                <TableColumn fx:id="tc_id" prefWidth="28.0" text="Id" />
                <TableColumn fx:id="tc_nome" prefWidth="132.0" text="Nome" />
                  <TableColumn fx:id="tc_cpf" text="Cpf" />
                  <TableColumn fx:id="tc_endereco" prefWidth="106.0" text="Endereço" />
                  <TableColumn fx:id="tc_projeto" prefWidth="136.0" text="Projeto" />
                  <TableColumn fx:id="tc_protocolo" minWidth="6.0" prefWidth="136.0" text="Protocolo" />
                  <TableColumn fx:id="tc_telefone" prefWidth="161.0" text="Telefone" />
                  <TableColumn fx:id="tc_email" minWidth="6.0" prefWidth="171.0" text="Email" />
              </columns>
            </TableView>
         </children>
      </AnchorPane>
   </children>
</HBox>
